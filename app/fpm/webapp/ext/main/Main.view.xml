<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:macros="sap.fe.macros"
    xmlns:html="http://www.w3.org/1999/xhtml" controllerName="sap.fe.cap.travel.fpm.ext.main.Main">
    <Page id="Main" title="{i18n>MainTitle}">
        <content>
        <macros:FilterBar
						metaPath="/Travel/@com.sap.vocabularies.UI.v1.SelectionFields"
						id="FilterBar"
						search=".handlers.onFiltersChanged"
						filterChanged=".handlers.onFiltersChanged"
					/>
        <macros:Table metaPath="/Travel/@com.sap.vocabularies.UI.v1.LineItem" id="LineItemTable" disableCopyToClipboard="true" />
      
        </content>
    </Page>
</mvc:View>